@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@model List<WTechECommerce.UI.Models.AdminUserVM>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Id</th>
      <th scope="col">Email</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      @foreach (var item in Model)
     {
     <tr>
      <th scope="row">1</th>
      <td>@item.Id</td>
      <td>@item.Email</td>
      <td><button class="btn btn-primary" type="submit">Update</button> <button class="btn btn-primary" type="submit">Delete</button></td>
    </tr>   
     }
  </tbody>
</table>